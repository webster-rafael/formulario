{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "public": {
      "rewrites": [
        {
          "source": "/public/(.*)",
          "destination": "/$1"
        }
      ]
    }
  }
  